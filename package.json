{
  "name": "ivy-ui",
  "version": "0.8.0",
  "description": "符合Web Component规范的前端组件库",
  "keywords": [
    "ivy-ui",
    "ivyui",
    "web component",
    "component",
    "ivy",
    "ui",
    "uikit"
  ],
  "main": "packages/ivy-ui/lib/index.js",
  "repository": "https://github.com/GuoJikun/fox-ui-wc.git",
  "author": "guojikun <guojikun@y-open.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "npm run lib & npm run docs",
    "lib": "npm run dev --filter ivy-ui",
    "docs": "npm run dev --filter example",
    "dev:lib": "npm run dev --dir=packages/ivy-ui",
    "dev:docs": "npm run dev --dir packages/example",
    "build:docs": "npm run build --dir packages/example",
    "build:lib": "npm run build --dir=packages/ivy-ui",
    "build": "rollup -c rollup.config.prod.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "lerna": "^4.0.0",
    "rollup": "^2.57.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
